<Window x:Class="Find_Factors.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Find_Factors"
        mc:Ignorable="d"
        Title="Enter a number to see its factors!!!" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <TextBox x:Name="tbxNumber" FontSize="50" TextChanged="tbxNumber_TextChanged">
        	
        </TextBox>
        <TextBlock x:Name="tbFactors" FontSize="50" Text="" Width="Auto" Height="Auto"/>
    </StackPanel>
</Window>
